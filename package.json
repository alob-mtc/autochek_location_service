{
  "name": "autochek-apis",
  "version": "0.0.1",
  "description": "description",
  "author": "Akinlua Bolamigbe",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "rm-yaml": "(rm ormconfig.yaml || :)",
    "ch:permit": "sudo chmod +x ./src/Scripts/run.config.sh",
    "sh-config:dev": "./src/Scripts/run.config.sh",
    "run:script_dev": "yarn rm-yaml && yarn ch:permit && yarn sh-config:dev",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/Main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "write:file": "ts-node src/Scripts/write.file.script.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn typeorm -- migration:generate -n AutoChek",
    "typeorm:migration:run": "yarn typeorm -- migration:run",
    "typeorm:migration:revert": "yarn typeorm -- migration:revert"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "faker": "^5.5.3",
    "form-my-simple-validation": "^0.0.11",
    "module-alias": "^2.2.2",
    "nestjs-typeorm-paginate": "^2.2.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.2",
    "write-yaml-file": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^5.5.6",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "_moduleAliases": {
    "@Root": "dist/",
    "@Model": "dist/Models",
    "@Controller": "dist/Controllers",
    "@Service": "dist/Services",
    "@Setting": "dist/Settings",
    "@Core": "dist/Modules",
    "@Constant": "dist/Constant"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@Root": "<rootDir>/../src/$1"
    },
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "type": "module"
  }
}
